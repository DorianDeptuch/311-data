{
  "family": "311-data-api",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "redis",
      "image": "redis",
      "portMappings": [
        {
          "containerPort": 6379
        }
      ],
      "essential": true
    },
    {
      "name": "app",
      "image": "docker.pkg.github.com/hackforla/311-data/api:dev",
      "portMappings": [
        {
          "containerPort": 5000
        }
      ],
      "essential": true,
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost/status/api"
        ],
        "interval": 20,
        "timeout": 30,
        "retries": 2
      },
      "environment": [
        { "name": "DEBUG", "value": "True" },
        { "name": "CACHE_ENDPOINT", "value": "localhost" },
        { "name": "DB_DSN", "value": "postgresql://311_user_dev:QDAQutA5FVVkAUTU@dbsvr-311-data.cs29ter7rwc4.us-east-1.rds.amazonaws.com:5432/311_db_dev" },
        { "name": "SENTRY_URL", "value": "https://467b42786c584f87ae7a7760f96b5652@o452210.ingest.sentry.io/5439266" }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256", 
  "memory": "512"
}
