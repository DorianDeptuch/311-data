{
  "family": "311-data-server-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::640613795150:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "redis",
      "image": "redis",
      "portMappings": [
        {
          "containerPort": 6379
        }
      ],
      "essential": true
    },
    {
      "name": "311_data_api",
      "image": "sapparisoftware/311_data_api",
      "portMappings": [
        {
          "containerPort": 5000
        }
      ],
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "python -c \"print('health OK')\""
        ],
        "interval": 20,
        "timeout": 30,
        "retries": 2
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group" : "/dev-cluster/311_data_api",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      },
      "environment": [
        { "name": "DEBUG", "value": "True" },
        { "name": "CACHE_ENDPOINT", "value": "localhost" },
        { "name": "DB_DSN", "value": "postgresql://311_user_dev:QDAQutA5FVVkAUTU@dbsvr-311-data.cs29ter7rwc4.us-east-1.rds.amazonaws.com:5432/311_db_dev" },
        { "name": "SENTRY_URL", "value": "https://467b42786c584f87ae7a7760f96b5652@o452210.ingest.sentry.io/5439266" }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256", 
  "memory": "512"
}
